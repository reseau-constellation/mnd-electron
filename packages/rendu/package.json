{
  "name": "@constl/mandataire-electron-rendu",
  "version": "2.3.0",
  "description": "Pour utiliser Constellation dans le processus rendu d'Électron. Doit être utilisé conjointement avec @constl/mandataire-electron-principal.",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "nettoyer": "rimraf dist",
    "compiler": "pnpm versioner && pnpm nettoyer && pnpm tspc -p tsconfig.build.json",
    "test": "echo \"Error: no test specified\" && exit 0",
    "couverture": "pnpm test -- --coverage",
    "couverture:tous": "pnpm test:tous -- --coverage --bail",
    "formatter": "pnpm prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\" && pnpm eslint src",
    "prepublish": "pnpm versioner && pnpm compiler",
    "versioner": "pnpm genversion  --es6 --semi src/version.ts"
  },
  "keywords": [],
  "author": "Julien Malard-Adam",
  "license": "AGPL-3.0-or-later",
  "dependencies": {
    "@constl/mandataire": "^2.0.8",
    "events": "^3.3.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@constl/ipa": "^1.1.1",
    "@constl/mandataire-electron-principal": "workspace:^",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^9.6.0",
    "eslint-config-prettier": "^9.1.0",
    "genversion": "^3.2.0",
    "prettier": "^3.3.2",
    "rimraf": "^5.0.7",
    "ts-patch": "^3.2.1",
    "typescript": "^5.5.3",
    "typescript-transform-paths": "^3.4.7"
  },
  "gitHead": "685c2d66c3509fbff802c25985478938ad178956"
}
